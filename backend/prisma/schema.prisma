generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Scan {
  uuid         String   @id
  url          String
  timestamp    DateTime
  status       String
  progress     Int
  spiderScanId String?
  activeScanId String?
  error        String?
}
